rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /logos/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /resumes/{allPaths=**} {
      allow read: if false;
      allow write: if request.auth != null && request.resource.size < 10 * 1024 * 1024;
    }
  }
}
